{
  "name": "react-webpack-rails-tutorial",
  "version": "0.0.1",
  "engines": {
    "node": "5.10.0",
    "npm": "3.5.0"
  },
  "scripts": {
    "build:client": "NODE_ENV=production webpack --config webpack.client.rails.build.config.js",
    "build:server": "NODE_ENV=production webpack --config webpack.server.rails.build.config.js",
    "build:dev:client": "babel-node server.rails.hot.js",
    "build:dev:server": "webpack -w --config webpack.server.rails.build.config.js",
    "build:test:client": "webpack -w --config webpack.client.rails.build.config.js",
    "test": "NODE_PATH=./app mocha --compilers js:babel-core/register --require ./app/lib/testHelper.js --require ./app/lib/testNullCompiler.js 'app/**/*.spec.@(js|jsx)'",
    "test:debug": "npm run test -- --debug-brk",
    "lint": "npm run eslint && npm run jscs",
    "eslint": "eslint --ext .js,.jsx .",
    "jscs": "jscs --verbose .",
    "hot-assets": "babel-node server.rails.hot.js"
  },
  "dependencies": {
    "autoprefixer": "^6.3.5",
    "axios": "^0.9.1",
    "babel": "^6.5.2",
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.4",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^6.6.1",
    "bootstrap": "^4.0.0-alpha.2",
    "bootstrap-loader": "^1.0.0-rc",
    "css-loader": "^0.23.1",
    "es5-shim": "^4.5.7",
    "expose-loader": "^0.7.1",
    "extract-text-webpack-plugin": "^0.9.1",
    "file-loader": "^0.8.4",
    "history": "^1.13.1",
    "immutable": "^3.7.6",
    "imports-loader": "^0.6.5",
    "isomorphic-fetch": "^2.2.1",
    "jquery": "^2.2.2",
    "jquery-ujs": "^1.2.1",
    "loader-utils": "^0.2.13",
    "lodash": "^4.7.0",
    "mirror-creator": "1.1.0",
    "node-sass": "^3.4.2",
    "postcss": "^5.0.21",
    "postcss-loader": "^0.8.2",
    "react": "^0.14.8",
    "react-dom": "^0.14.8 || ^15.0.0",
    "react-on-rails": "5.2.0",
    "react-redux": "^4.4.1",
    "redux": "^3.3.1",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.0.1",
    "resolve-url-loader": "^1.4.3",
    "sass-loader": "^3.1.1",
    "sass-resources-loader": "0.0.2",
    "style-loader": "^0.13.0",
    "tether": "^1.3.2",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.14"
  },
  "devDependencies": {
    "babel-eslint": "^5.0.0-beta6",
    "babel-plugin-react-transform": "^2.0.0-beta1",
    "body-parser": "^1.14.1",
    "chai": "^3.4.1",
    "chai-immutable": "^1.5.3",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^2.0.0",
    "eslint-plugin-react": "^3.11.3",
    "express": "^4.13.3",
    "jade": "^1.11.0",
    "jscs": "^2.7.0",
    "jsdom": "^7.0.2",
    "mocha": "^2.3.4",
    "react-addons-test-utils": "^0.14.3",
    "react-transform-hmr": "^1.0.1",
    "sleep": "^3.0.0",
    "webpack-dev-server": "^1.12.1"
  }
}
